@model ClientLibViewModel;
@{
    ViewData["Title"] = "Users List";
}
<h1> View check</h1>

<div class="container">
    <table class="table table-hover">
        <thead>
            <tr>

                <td>UserId</td>
                <td>Email</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Usercolection)
            {
                <tr>

                    <td>@item.UserId</td>
                    <td>@item.Email</td>
                </tr>
            }
        </tbody>
    </table>
</div>